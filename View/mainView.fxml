<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="471.0" prefWidth="940.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.mainController">
  <children>
      <MenuBar>

      </MenuBar>
    <AnchorPane fx:id="mainAnchorPage" maxHeight="-1.0" maxWidth="-1.0" prefHeight="478.0" prefWidth="954.0" VBox.vgrow="ALWAYS">
      <children>
            <AnchorPane fx:id="partsAnchorPage" layoutX="17.0" layoutY="69.0" prefHeight="306.0" prefWidth="443.0" style="-fx-border-style: solid; -fx-border-radius: 20;">
               <children>
              <Label fx:id="customersLabel" alignment="CENTER" layoutX="27.0" layoutY="27.0" style="&#10;" text="Customers" textAlignment="CENTER" wrapText="false">
                <font>
                  <Font name="System Bold" size="14.0" />
                </font>
              </Label>
                  <TextField fx:id="customersSearchText" layoutX="250.0" layoutY="25.0" onKeyReleased="#onKeyReleasedCustomerSearchText" prefHeight="25.0" prefWidth="157.0" promptText="Search by ID or Name" />
                  <TableView fx:id="partTable" layoutX="13.0" layoutY="57.0" prefHeight="196.0" prefWidth="410.0">
                    <columns>
                      <TableColumn fx:id="customerIDTableColumn" prefWidth="75.0" text="Customer ID" />
                      <TableColumn fx:id="customerNameTableColumn" prefWidth="108.0" text="Name" />
                        <TableColumn fx:id="customerAddressTableColumn" prefWidth="106.0" text="Address" />
                        <TableColumn fx:id="customerPhoneTableColumn" prefWidth="119.0" text="Phone" />
                    </columns>
                  </TableView>
                  <Button fx:id="addCustomerButton" layoutX="227.0" layoutY="269.0" mnemonicParsing="false" onAction="#addCustomersButtonAction" prefHeight="25.0" prefWidth="54.0" text="Add" />
                  <Button fx:id="editCustomerButton" layoutX="291.0" layoutY="269.0" mnemonicParsing="false" onAction="#editCustomerButtonAction" prefHeight="25.0" prefWidth="54.0" text="Edit" />
                  <Button fx:id="deleteCustomerButton" layoutX="356.0" layoutY="269.0" mnemonicParsing="false" onAction="#deleteCustomersButtonAction" text="Delete" />
               </children>
            </AnchorPane>
            <Label fx:id="mainLabel" alignment="CENTER" layoutY="29.0" prefHeight="20.0" prefWidth="940.0" text="Main Dashboard">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <AnchorPane fx:id="productAnchorPage" layoutX="477.0" layoutY="68.0" prefHeight="306.0" prefWidth="443.0" style="-fx-border-style: solid; -fx-border-radius: 20;">
               <children>
                  <Label fx:id="appointmentsLabel" alignment="CENTER" layoutX="27.0" layoutY="27.0" style="&#10;" text="Appointments" textAlignment="CENTER" wrapText="false">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Label>
                  <TextField fx:id="appointmentsSearchText" layoutX="250.0" layoutY="25.0" onKeyReleased="#onKeyReleasedAppointmentSearchText" prefHeight="25.0" prefWidth="157.0" promptText="Search by ID or Name" />
                  <TableView fx:id="productTable" layoutX="13.0" layoutY="57.0" prefHeight="196.0" prefWidth="410.0">
                     <columns>
                        <TableColumn fx:id="appointmentIDTableColumn" prefWidth="75.0" text="ID" />
                        <TableColumn fx:id="appointmentStartTableColumn" prefWidth="108.0" text="Start Time" />
                        <TableColumn fx:id="appointmentEndTableColumn" prefWidth="106.0" text="End Time" />
                        <TableColumn fx:id="appointmentLocationTableColumn" prefWidth="119.0" text="Location" />
                     </columns>
                  </TableView>
                  <Button fx:id="productAddButton" layoutX="237.0" layoutY="269.0" mnemonicParsing="false" onAction="#addAppointmentsButtonAction" text="Add" />
                  <Button fx:id="productModifyButton" layoutX="289.0" layoutY="269.0" mnemonicParsing="false" onAction="#editAppointmentButtonAction" text="Modify" />
                  <Button fx:id="productDeleteButton" layoutX="356.0" layoutY="269.0" mnemonicParsing="false" onAction="#deleteAppointmentButtonAction" text="Delete" />
               </children>
            </AnchorPane>
            <Button fx:id="logoutButton" layoutX="806.0" layoutY="396.0" mnemonicParsing="false" onAction="#logoutButtonAction" text="Log out" />
      </children>
    </AnchorPane>
  </children>
</VBox>
